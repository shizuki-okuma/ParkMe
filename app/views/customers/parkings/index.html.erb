<div class="container sm-mt-5 mb-5">
  <div class="row">
    <div class="col-sm-12">
      <h3 class="col-sm-mb-4"><strong>駐車場一覧</strong></h3>
      <!--検索フォーム-->
      <div><%= render 'searches/search' %></div>
      <% @parkings.each do |parking| %>
        <table class="table table-borderless">
          <tr>
            <td><%= link_to customers_parking_path(parking.id) do %>
              <%= parking.name %>
            <% end %>
            </td>
          </tr>
        <% end %>
      </table>
    </div>
  </div>
</div>
  