<div class="container" style="height: 600px;">
  <div class="row">
    <div class="col-sm-12">
      <%= render 'searches/search' %>

      <table class="table table-borderless mb-0">
      <% if @range == "駐車場名" || @range == "郵便番号" %>
        <tbody>
          <% @parkings.each do |parking| %>
            <tr class="clickable-row" onclick='window.location="/customers/parkings/<%= parking.id %>"' role="link">
              <td><%= parking.name %></td>
            </tr>
          <% end %>
        </tbody>
      <% else %>
        <!--検索対象モデルがparkingではない時(= 検索対象モデルがcar_modelの時) -->
        <tbody>
          <%# @car_models.each do |car_model| %>
            <tr>
              <td><%#= car_model.name %></td>
            </tr>
          <%# end %>
        </tbody>
      <% end %>
      </table>
      
    </div>
  </div>
</div>